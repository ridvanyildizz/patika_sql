--  Sütunların her birine göre diğer sütunları güncelleyecek 5 adet UPDATE işlemi yapalım.

--
UPDATE employee
SET  first_name = 'RIDVAN',
     last_name = 'YILDIZ',
    email = 'ryildiz@yildiz.com'
WHERE id = 5
RETURNING * ;

--

UPDATE employee
SET  last_name = 'YILDIZ',
WHERE id = 12
RETURNING * ;

--
UPDATE employee
SET  email = 'rdvan@yildiz.com',
WHERE id = 20
RETURNING * ;

--
UPDATE employee
SET  first_name = 'YILDIZ',
     birthday = '1990-01-01',
WHERE id = 40
RETURNING * ;

--
UPDATE employee
SET  first_name = 'RIDVAN',
     last_name = 'YILDIZ',
     birthday = '1990-01-01',
     email = 'yildiz@ridvan.com',
     
WHERE id = 22
RETURNING * ;

--
-- Sütunların her birine göre ilgili satırı silecek 5 adet DELETE işlemi yapalım.

DELETE FROM employee
WHERE name LIKE '%Y'
RETURNING *;

--

DELETE FROM employee
WHERE email ILIKE '%x%'
RETURNING *;

--

DELETE FROM employee
WHERE id BETWEEN 37 AND 40
RETURNING *;

--

DELETE FROM employee
WHERE id = 12
RETURNING *;

--

DELETE FROM employee
WHERE birthday > '01-01-1999'
returning *;

--

Display final state of the employee table
SELECT * FROM employee;















