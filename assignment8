
-- Test veritabanınızda employee isimli sütun bilgileri id(INTEGER), name VARCHAR(50), 
--     birthday DATE, email VARCHAR(100) olan bir tablo oluşturalım.

CREATE TABLE employee (
	id INT NOT NULL, 
	name VARCHAR(50) NOT NULL,
	birthday DATE,
	email VARCHAR(100)
  
  );


